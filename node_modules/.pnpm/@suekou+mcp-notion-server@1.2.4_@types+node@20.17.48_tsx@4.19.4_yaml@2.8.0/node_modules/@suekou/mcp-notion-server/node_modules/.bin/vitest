#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules/vitest/node_modules:/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules:/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules/vitest/node_modules:/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules:/Users/harshsinghal/Desktop/Vibe Coding/Random Tools/Notion Extension/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@3.0.9_@types+node@20.17.48_tsx@4.19.4_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
fi
