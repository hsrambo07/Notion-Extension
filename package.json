{
  "name": "notion-agent-extension",
  "version": "1.0.0",
  "description": "Chrome extension for Notion Agent",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cross-env MCP_NOTION_PORT=3333 npx @suekou/mcp-notion-server\" \"tsx server/server.ts\"",
    "build": "tsc && esbuild extension/popup.ts --bundle --outfile=extension/popup.js",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest run",
    "test:whatsapp": "cross-env NODE_ENV=test tsx server/whatsapp/test.ts",
    "prepare": "husky"
  },
  "keywords": [
    "notion",
    "chrome-extension",
    "agent"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "@suekou/mcp-notion-server": "latest",
    "dotenv": "^16.4.5",
    "hono": "^4.0.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
    "spinai": "^1.0.0",
    "zod": "^3.22.4",
    "@hono/utils": "^1.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "nock": "^13.4.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{ts,tsx,json,md}": "prettier --write"
  }
}
